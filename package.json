{
  "name": "botmover",
  "version": "1.0.0",
  "description": "## setup",
  "main": "index.js",
  "scripts": {
    "test": "node tests/index.js",
    "insert-blank-lines": "yes \"\" | head -50",
    "watch-tests": "npm test; chokidar './*.js' 'lib/**/*.js' 'tests/**/*.js' -c 'npm run insert-blank-lines; npm test'"
  },
  "engines": {
    "node": ">=8.6.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/r-k-b/botmover.git"
  },
  "author": "Robert K. Bell <robert.k.bell@gmail.com>",
  "license": "GPL-3.0",
  "bugs": {
    "url": "https://github.com/r-k-b/botmover/issues"
  },
  "homepage": "https://github.com/r-k-b/botmover#readme",
  "dependencies": {
    "chokidar-cli": "1.2.0",
    "inquirer": "3.3.0",
    "prettier": "1.7.3",
    "tape": "4.8.0"
  },
  "prettier": {
    "bracketSpacing": false,
    "singleQuote": true,
    "semi": false,
    "trailingComma": "all"
  }
}
